<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>Exe</OutputType>
        <TargetFramework>net9.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>

        <PackAsTool>true</PackAsTool>
        <ToolCommandName>godotNodeInstall</ToolCommandName>
        <PackageOutputPath>./nupkg</PackageOutputPath>
        <Authors>stark233</Authors>
        <Description>GodotNodeExtension install CLI Tool.</Description>
        <Copyright>GodotNodeExtension</Copyright>
        <PackageProjectUrl>https://github.com/GodotNodeExtension/GodotNodeExtensionInstaller</PackageProjectUrl>
        <RepositoryUrl>https://github.com/GodotNodeExtension/GodotNodeExtensionInstaller</RepositoryUrl>
        <Version>1.0.1</Version>
        <Title>GodotNodeExtensionInstaller</Title>
        <PackageLicenseUrl></PackageLicenseUrl>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="ConsoleAppFramework" Version="5.5.0">
        <PrivateAssets>all</PrivateAssets>
        <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
      </PackageReference>
    </ItemGroup>

</Project>
